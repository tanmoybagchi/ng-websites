{
  "name": "repo",
  "version": "1.0.0",
  "scripts": {
    "build-lib": "run-s -c clean-lib build-core build-gapi build-mh build-material-cms",
    "clean-lib": "rmdir /s/q dist",
    "build-core": "ng build core",
    "build-gapi": "ng build gapi",
    "build-mh": "run-p build-mh-*",
    "build-mh-general-error": "ng build mh-general-error",
    "build-mh-google-sign-in": "ng build mh-google-sign-in",
    "build-mh-input-date": "ng build mh-input-date",
    "build-mh-input-email": "ng build mh-input-email",
    "build-mh-input-file": "ng build mh-input-file",
    "build-mh-input-integer": "ng build mh-input-integer",
    "build-mh-input-search": "ng build mh-input-search",
    "build-mh-input-select": "ng build mh-input-select",
    "build-mh-input-text": "ng build mh-input-text",
    "build-mh-input-usd": "ng build mh-input-usd",
    "build-mh-page-title": "ng build mh-page-title",
    "build-mh-server-error": "ng build mh-server-error",
    "build-mh-throbber": "ng build mh-throbber",
    "build-material-cms": "run-s build-material-cms-*",
    "build-material-cms-view": "ng build material-cms-view",
    "build-material-cms-admin": "ng build material-cms-admin",
    "build-material-cms-providers": "ng build material-cms-providers",
    "ngc-cms-a": "ng g component --project=material-cms-admin --",
    "ngc-cms-v": "ng g component --project=material-cms-view --",
    "ngc-dsl": "ng g component --project=DailySpendLimit --",
    "ngc-rr": "ng g component --project=RedditReader --",
    "ngc-vtw": "ng g component --project=VinesToWinesDM --",
    "ngm-dsl": "ng g module --project=DailySpendLimit --",
    "ngm-rr": "ng g module --project=RedditReader --",
    "ngm-vtw": "ng g module --project=VinesToWinesDM --",
    "serve-dsl": "ng s DailySpendLimit",
    "serve-sc": "ng s StavesChurch",
    "serve-rr": "ng s RedditReader",
    "serve-tk": "ng s TiffanysKitchen",
    "serve-vtw": "ng s VinesToWinesDM",
    "build-dsl": "ng b DailySpendLimit --prod --output-path=../DailySpendLimit/docs/ && copy ..\\DailySpendLimit\\docs\\index.html ..\\DailySpendLimit\\docs\\404.html",
    "build-sc-dev": "ng b StavesChurch --configuration=dev --noprogress --output-path=C:/inetpub/wwwroot/StavesChurch/docs/ --base-href=/StavesChurch/",
    "build-sc-test": "ng b StavesChurch --prod --noprogress --output-path=../StavesChurch/docs/ --base-href=/StavesChurch/ && copy ..\\StavesChurch\\docs\\index.html ..\\StavesChurch\\docs\\404.html",
    "build-sc": "ng b StavesChurch --prod --noprogress --output-path=../StavesChurch-Prod/ --base-href=/",
    "build-rr": "git -C ..\\RedditReader pull & node projects\\RedditReader\\replace.build.js projects/RedditReader/src/environments/environment.prod.ts && ng b RedditReader --prod --output-path=../RedditReader/docs/ && copy ..\\RedditReader\\docs\\index.html ..\\RedditReader\\docs\\404.html && git -C ..\\RedditReader commit",
    "build-tk": "ng b TiffanysKitchen --prod --noprogress --output-path=../TiffanysKitchen/docs/ --base-href=/TiffanysKitchen/ && copy ..\\TiffanysKitchen\\docs\\index.html ..\\TiffanysKitchen\\docs\\404.html",
    "build-tk-dev": "ng b TiffanysKitchen --configuration=dev --noprogress --output-path=C:/inetpub/wwwroot/TiffanysKitchen/docs/ --base-href=/TiffanysKitchen/",
    "build-vtw-dev": "ng b VinesToWinesDM --configuration=dev --noprogress --output-path=C:/inetpub/wwwroot/VinesToWinesDM/docs/ --base-href=/VinesToWinesDM/",
    "build-vtw-test": "ng b VinesToWinesDM --prod --noprogress --output-path=../VinesToWinesDM/docs/ --base-href=/VinesToWinesDM/ && copy ..\\VinesToWinesDM\\docs\\index.html ..\\VinesToWinesDM\\docs\\404.html",
    "build-vtw": "ng b VinesToWinesDM --prod --noprogress --output-path=../VinesToWinesDM-Prod/ --base-href=/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "github:angular/flex-layout-builds",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.803.18",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "hammerjs": "^2.0.8",
    "jsrsasign": "^10.2.0",
    "material-icon-font": "^3.0.1",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~6.4.0",
    "shaka-player": "^2.5.6",
    "tslib": "^1.10.0",
    "typeface-great-vibes": "0.0.72",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular-devkit/build-ng-packagr": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "replace-in-file": "^4.2.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
